{
  "name": "@dmac/mcp-control",
  "version": "1.0.0",
  "description": "Enhanced tooling orchestration for MCP (Model Context Protocol) servers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
  "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
  "prebuild": "npm run clean",
  "svc:install": "pwsh -File ./scripts/install-windows-service.ps1 -ServiceName MCPControl -Env production",
  "svc:uninstall": "pwsh -File ./scripts/uninstall-windows-service.ps1 -ServiceName MCPControl"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "orchestration",
    "tooling",
    "ai",
    "automation"
  ],
  "author": "Dmac",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/websocket": "^8.3.1",
    "@types/node": "^20.19.12",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "eventsource": "^2.0.2",
    "fastify": "^4.29.1",
    "lodash": "^4.17.21",
    "pino": "^8.16.2",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.202",
    "@types/node-fetch": "^2.6.2",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "node-fetch": "^2.6.7",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.0",
  "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jluna0413/Dmac-alt.git"
  }
}
